<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/buttonA1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@id/guideline_bord_v1"
        app:layout_constraintEnd_toStartOf="@+id/guideline_bord_v2"
        app:layout_constraintTop_toBottomOf="@id/guideline_bord_h1"
        app:layout_constraintBottom_toTopOf="@+id/guideline_bord_h2"
        app:srcCompat="@drawable/mass" />

    <ImageView
        android:id="@+id/buttonB1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@id/guideline_bord_v3"
        app:layout_constraintEnd_toStartOf="@+id/guideline_bord_v4"
        app:layout_constraintTop_toBottomOf="@id/guideline_bord_h1"
        app:layout_constraintBottom_toTopOf="@+id/guideline_bord_h2"
        app:srcCompat="@drawable/mass" />

    <ImageView
        android:id="@+id/buttonC1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@id/guideline_bord_v5"
        app:layout_constraintEnd_toStartOf="@+id/guideline_bord_v6"
        app:layout_constraintTop_toBottomOf="@id/guideline_bord_h1"
        app:layout_constraintBottom_toTopOf="@+id/guideline_bord_h2"
        app:srcCompat="@drawable/mass" />

    <ImageView
        android:id="@+id/buttonA2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@id/guideline_bord_v1"
        app:layout_constraintEnd_toStartOf="@+id/guideline_bord_v2"
        app:layout_constraintTop_toBottomOf="@id/guideline_bord_h3"
        app:layout_constraintBottom_toTopOf="@+id/guideline_bord_h4"
        app:srcCompat="@drawable/mass" />

    <ImageView
        android:id="@+id/buttonB2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@id/guideline_bord_v3"
        app:layout_constraintEnd_toStartOf="@+id/guideline_bord_v4"
        app:layout_constraintTop_toBottomOf="@id/guideline_bord_h3"
        app:layout_constraintBottom_toTopOf="@+id/guideline_bord_h4"
        app:srcCompat="@drawable/mass" />

    <ImageView
        android:id="@+id/buttonC2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@id/guideline_bord_v5"
        app:layout_constraintEnd_toStartOf="@+id/guideline_bord_v6"
        app:layout_constraintTop_toBottomOf="@id/guideline_bord_h3"
        app:layout_constraintBottom_toTopOf="@+id/guideline_bord_h4"
        app:srcCompat="@drawable/mass" />

    <ImageView
        android:id="@+id/buttonA3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@id/guideline_bord_v1"
        app:layout_constraintEnd_toStartOf="@+id/guideline_bord_v2"
        app:layout_constraintTop_toBottomOf="@id/guideline_bord_h5"
        app:layout_constraintBottom_toTopOf="@+id/guideline_bord_h6"
        app:srcCompat="@drawable/mass" />

    <ImageView
        android:id="@+id/buttonB3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@id/guideline_bord_v3"
        app:layout_constraintEnd_toStartOf="@+id/guideline_bord_v4"
        app:layout_constraintTop_toBottomOf="@id/guideline_bord_h5"
        app:layout_constraintBottom_toTopOf="@+id/guideline_bord_h6"
        app:srcCompat="@drawable/mass" />

    <ImageView
        android:id="@+id/buttonC3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintStart_toEndOf="@id/guideline_bord_v5"
        app:layout_constraintEnd_toStartOf="@+id/guideline_bord_v6"
        app:layout_constraintTop_toBottomOf="@id/guideline_bord_h5"
        app:layout_constraintBottom_toTopOf="@+id/guideline_bord_h6"
        app:srcCompat="@drawable/mass" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_bord_v1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_bord_v2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.34" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_bord_v3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.38" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_bord_v4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.62" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_bord_v5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.66" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_bord_v6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.9" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_bord_h1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.25" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_bord_h2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.39" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_bord_h3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.41" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_bord_h4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.56" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_bord_h5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.58" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_bord_h6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.71" />

</androidx.constraintlayout.widget.ConstraintLayout>