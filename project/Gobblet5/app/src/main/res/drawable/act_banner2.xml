<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <stroke
        android:width="5dp"
        android:color="#f00"
        android:dashGap="5dp"
        android:dashWidth="10dp" />
    <item
        android:width="200dp"
        android:height="100dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFBD6"/>
        </shape>
    </item>

    <item
        android:drawable="@drawable/pin"
        android:width="40dp"
        android:height="40dp"
        android:start="90dp"
        android:top="-20dp"
        />

</layer-list>